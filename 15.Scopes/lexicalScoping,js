var teacher = "sanket";
function fun() {
    var teacher = "pulkit"
    console.log(teacher);
}
fun();

// Difference between function scope and global
var teacher = "sanket";
function fun() {
    var teacher = "pulkit"
    console.log(teacher);//calls scope of fun teacher variable
}
console.log(teacher);//calls Global scope  teacher variable
fun();



/** Example given to clarify more   */
function fun(val) {
    var student;
}
function run() {
    console.log(student);
}
student = 10;
function gun() {
    console.log(student);
    var student = 100;
}
gun();
run(); 